    </None>
    <None Update="Games\Dark Souls I\Restrictions.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Games\Dark Souls I\Submission.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Games\Demon%27s Souls\Bosses.txt">
